{
    "import": ["@cspell/dict-lorem-ipsum/cspell-ext.json"],
    "caseSensitive": false,
    "dictionaries": ["custom-words"],
    "dictionaryDefinitions": [
        {
            "name": "custom-words",
            "path": ".cspell/custom-words.txt",
            "addWords": true
        }
    ],
    "ignorePaths": [
        "**/node_modules/**",
        "**/dist/**",
        "**/lib/**",
        "**/docs/**",
        "**/stats.html",
        "**/language/**",
        "**/language.ts",
        "**/package.json",
        "**/website/**",
        "eslint.config.js"
    ],
    "ignoreWords": [
        "Cascader",
        "Consolas",
        "Menlo",
        "Neue",
        "Noto",
        "Segoe",
        "alicdn",
        "cssinjs",
        "cssinjsutils",
        "cthb",
        "favicons",
        "geekblue",
        "maxmin",
        "mfsu",
        "motion",
        "outter",
        "sillyvg",
        "stylis",
        "suoxiao",
        "unitless",
        "wireframe",
        "sillyvg"
    ]
}
